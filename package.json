{
  "name": "node-hid",
  "description": "USB HID device access library",
  "version": "0.5.1",
  "author": "Hans HÃ¼bner <hans.huebner@gmail.com> (https://github.com/hanshuebner)",
  "contributors": [
    "Blake Miner <miner.blake@gmail.com>",
