  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/node-hid/node-hid.git"
  },
  "scripts": {
    "test": "node nodehid.js",
    "prepublish": "git submodule update --init",
    "preinstall": "git submodule update -i -r",
    "install": "node-pre-gyp install --build-from-source"
  },
  "binary": {
    "module_name": "HID",
