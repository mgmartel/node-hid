    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://node-hid.s3.amazonaws.com"
  },
  "main": "./nodehid.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "(MIT OR X11)",
  "dependencies": {
    "nan": "^2.0.9",
    "node-pre-gyp": "0.6.x"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "aws-sdk": "^2.2.9"
  }
}
